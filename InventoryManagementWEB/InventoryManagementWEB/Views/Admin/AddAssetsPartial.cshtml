
<div class="col-md-8 header-bot-right" id="ADD_ASSETS" style="width:100%;">
    <!--analytic start here-->
    <!-- Login start here -->
    
        <h4 style="color:black;size:20px;text-align:center;padding-top:2%;">ADD ASSET SECTION</h4>
       
    <div class="modal-body">
        <form>
            <div class="form-group">
                <label for="exampleSelect1">ASSET NAME</label>
                <select class="form-control" id="exampleSelect1">
                    <option selected="Select" onclick="">Select an Asset</option>
                    <option>Laptop</option>
                    <option>HeadSet</option>
                    <option>Keyboard</option>
                    <option>WirelessMouse</option>
                    <option>Mobile</option>
                </select>
            </div>
            <div class="form-group">
                <label for="exampleSelect1">ASSET MANUFACTURER</label>
                <select class="form-control" id="exampleSelect1">
                    <option selected="Select" onclick="">Select an option</option>
                    <option>Dell</option>
                    <option>Hp</option>
                    <option>Sony</option>
                    <option>Intex</option>
                    <option>Acer</option>
                </select>
            </div>
            <div class="form-group">
                <label for="formGroupExampleInput">Asset ID</label>
                <input type="text" class="form-control" id="formGroupExampleInput" placeholder="Enter the AssetID">
            </div>
            <div class="form-group">
                <label for="example-datetime-local-input">Brought Date</label>

                <input class="form-control" type="datetime-local" placeholder="Select Date" value="" id="example-datetime-local-input">

            </div>
            <div class="form-group">
                <label for="exampleSelect1">WARRANTY PERIOD</label>
                <select class="form-control" id="exampleSelect1">
                    <option selected="Select" onclick="">Select No of Years</option>
                    <option>1</option>
                    <option>2</option>
                    <option>3</option>
                    <option>4</option>
                    <option>5</option>
                    <option>5 and more</option>
                </select>
            </div>
            <div class="form-group">
                <label for="exampleTextarea">FILL THE SPECIFICATIONS</label>
                <br>
                <br>
                <br>
                <label for="RAM">RAM</label>
                <select class="form-control" id="RAM">
                    <option selected="Select" onclick="">Select RAM in GB</option>
                    <option>1 GB</option>
                    <option>2 GB</option>
                    <option>4 GB</option>
                    <option>8 GB</option>
                    <option>16 GB</option>
                    <option>32 GB</option>
                    <option>64 GB</option>
                    <option>64 GB and more</option>
                </select>
                <label for="HardDisk">HardDisk Space</label>
                <input type="text" class="form-control" id="HardDisk" placeholder="Enter Hard Disk Space">
                <label for="Processor">Processor</label>
                <input type="text" class="form-control" id="Processor" placeholder="Enter Hard Disk Space">
            </div>

        </form>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary">Save changes</button>
    </div>
       


